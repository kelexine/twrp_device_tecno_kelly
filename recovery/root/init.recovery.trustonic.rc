# Start Daemon (Registry directories should already be present)
service mobicore /system/bin/mcDriverDaemon --P1 /mnt/vendor/persist/mcRegistry \
	-r /vendor/app/mcRegistry/020f0000000000000000000000000000.drbin \ # utils
	-r /vendor/app/mcRegistry/05120000000000000000000000000000.drbin \ # sec
	-r /vendor/app/mcRegistry/05140000000000000000000000000000.drbin \ # devinfo
	-r /vendor/app/mcRegistry/020b0000000000000000000000000000.drbin \ # cmdq
	-r /vendor/app/mcRegistry/05070000000000000000000000000000.drbin \ # goodix_fp
	-r /vendor/app/mcRegistry/07090000000000000000000000000000.drbin \ # sunwave_fp
	-r /vendor/app/mcRegistry/030b0000000000000000000000000000.drbin \ # spi
	-r /vendor/app/mcRegistry/030c0000000000000000000000000000.drbin \ # spi1
	-r /vendor/app/mcRegistry/07060000000000000000000000007169.drbin \ # MtkH264Vdec
	-r /vendor/app/mcRegistry/40188311faf343488db888ad39496f9a.drbin \ # widevine
	-r /vendor/app/mcRegistry/06090000000000000000000000000000.drbin \ # drm keyinstall
	-r /vendor/app/mcRegistry/5020170115e016302017012521300000.drbin \ # drm_hdcp_common
        user root
        group root
        class core
        priority -20
        seclabel u:r:recovery:s0
